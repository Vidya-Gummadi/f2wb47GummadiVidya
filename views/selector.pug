extends layout

block content
  h1= title
  - var image_names = ["Queen1.png", "Queen2.png", "Queen3.png", "Queen4.png", "Queen5.png"];
    - for(let i = 0; i < 3; i++) {
        - let ranValue = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[ranValue]}`, alt="Queen Image", height="300px", width="300px")
    - }